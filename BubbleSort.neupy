Program bubbleSort;
Var arr[10] : Int;
Var    n : Int;
Void Function  sort(arr : Int, n : Int)

Var    i : Int;
Var    j : Int;
Var    temp : Int;
{
    From (i = 0) To n-1 Do
    {
        From (j = 0) To n-i-1 Do
        {
            If (arr[j] > arr[j+1]) Then
            {
                temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            }
        }
    }
}
Main()
{
    # Read n and arr from input
    sort(arr, n);
    # Write arr to output
}
